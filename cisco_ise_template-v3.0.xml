<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2022-02-07T10:25:55Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Cisco_ISE</template>
            <name>Cisco_ISE</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <items>
                <item>
                    <name>Authentifications in last 15 min</name>
                    <type>EXTERNAL</type>
                    <key>ise.py[{$IPADDR},{$USER},{$PW},active]</key>
                    <delay>30s</delay>
                    <triggers>
                        <trigger>
                            <expression>{last(#5)}&lt;1</expression>
                            <name>No authentifications in last 5 min on {HOST.NAME}</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Registred sessions</name>
                    <type>EXTERNAL</type>
                    <key>ise.py[{$IPADDR},{$USER},{$PW},count]</key>
                    <delay>30s</delay>
                    <triggers>
                        <trigger>
                            <expression>{last(#5)}&lt;30</expression>
                            <name>Few auth sessions on {HOST.NAME}</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <macros>
                <macro>
                    <macro>{$IPADDR}</macro>
                    <value>0.0.0.0</value>
                </macro>
                <macro>
                    <macro>{$PW}</macro>
                    <value>password</value>
                </macro>
                <macro>
                    <macro>{$USER}</macro>
                    <value>monitor</value>
                </macro>
            </macros>
        </template>
    </templates>
    <graphs>
        <graph>
            <name>ISE_SESSIONS</name>
            <graph_items>
                <graph_item>
                    <color>199C0D</color>
                    <item>
                        <host>Cisco_ISE</host>
                        <key>ise.py[{$IPADDR},{$USER},{$PW},active]</key>
                    </item>
                </graph_item>
                <graph_item>
                    <sortorder>1</sortorder>
                    <color>F63100</color>
                    <item>
                        <host>Cisco_ISE</host>
                        <key>ise.py[{$IPADDR},{$USER},{$PW},count]</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
    </graphs>
</zabbix_export>
